{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/App.js",  
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",  
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",  
      "dest": "/src/$1"
    }
  ]
}
